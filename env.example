# Backend base URL (production default)
VERCEL_BASE_URL=https://orubacoin.com

# Worker token shared with Oruba backend
WORKER_API_TOKEN=replace-with-worker-token

# Token used by backend to authorize alert triggers
ALERT_TRIGGER_TOKEN=replace-with-trigger-token

# Optional: override tracked symbols (comma-separated)
# BINANCE_SYMBOLS=btcusdt,ethusdt

# Optional: override Binance WebSocket base URL
# BINANCE_WS_URL=wss://stream.binance.com:9443

# Alert refresh frequency in milliseconds
ALERT_REFRESH_INTERVAL_MS=60000

# Logging verbosity: error | warn | info | debug
LOG_LEVEL=info

